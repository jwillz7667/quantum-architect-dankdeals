{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "// Development": "",
    "dev": "vite",
    "dev:host": "vite --host",
    "dev:https": "vite --https",
    "preview": "vite preview",
    "preview:https": "vite preview --https",
    "// Build": "",
    "prebuild": "",
    "build": "npm run build:prod",
    "build:prod": "cross-env NODE_ENV=production vite build && npm run sitemap:prod",
    "build:dev": "cross-env NODE_ENV=development vite build --mode development",
    "build:staging": "cross-env NODE_ENV=staging vite build --mode staging",
    "build:production": "node scripts/build-production.js",
    "postbuild": "npm run build:size || echo 'Size limit check failed but build completed'",
    "build:size": "size-limit",
    "// Quality Checks": "",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint . --cache --cache-location .cache/eslint",
    "lint:styles": "stylelint \"src/**/*.{css,scss}\" --cache --cache-location .cache/stylelint || true",
    "lint:fix": "eslint . --fix && stylelint \"src/**/*.{css,scss}\" --fix || true",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "validate": "npm run type-check && npm run lint && npm run format:check",
    "// Testing": "",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.integration.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:smoke": "playwright test --grep @smoke",
    "test:a11y": "playwright test --grep @a11y",
    "// Performance": "",
    "analyze": "cross-env ANALYZE=true vite build",
    "analyze:deps": "depcheck",
    "lighthouse": "npm run lighthouse:local",
    "lighthouse:ci": "lhci autorun",
    "lighthouse:local": "node scripts/lighthouse-server.js & sleep 5 && lhci autorun --collect.startServerCommand='' --collect.url='http://localhost:5000' && kill %1",
    "// Security": "",
    "security": "npm audit --production",
    "security:fix": "npm audit fix --production",
    "deps:check": "npm outdated",
    "deps:update": "npm update",
    "deps:clean": "npx depcheck",
    "// Utilities": "",
    "clean": "rm -rf dist coverage test-results playwright-report .cache",
    "clean:all": "npm run clean && rm -rf node_modules",
    "sitemap": "npm run sitemap:prod",
    "sitemap:dev": "cross-env NODE_ENV=development node scripts/generate-sitemap.js",
    "sitemap:prod": "cross-env NODE_ENV=production node scripts/generate-sitemap.js",
    "env:validate": "node scripts/validate-env.js",
    "prepare": "node -e \"try { require.resolve('husky'); require('child_process').execSync('husky', {stdio: 'inherit'}); } catch (e) {}\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@supabase/supabase-js": "^2.53.0",
    "@tanstack/react-query": "^5.84.1",
    "@types/dompurify": "^3.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-fetch": "^4.1.0",
    "date-fns": "^3.6.0",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.536.0",
    "next-themes": "^0.4.6",
    "ra-input-rich-text": "^5.10.1",
    "ra-supabase": "^3.5.0",
    "react": "^18.3.1",
    "react-admin": "^5.10.1",
    "react-dom": "^18.3.1",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^6.24.1",
    "sonner": "^1.5.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76"
  },
  "optionalDependencies": {
    "@axe-core/react": "^4.10.2",
    "@vercel/analytics": "^1.5.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.0",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@eslint/js": "^9.31.0",
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.48.0",
    "@size-limit/file": "^11.2.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^20.14.10",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-virtualized-auto-sizer": "^1.0.4",
    "@types/react-window": "^1.8.8",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@typescript-eslint/parser": "^8.13.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "browserslist": "^4.24.4",
    "caniuse-api": "^3.0.0",
    "cross-env": "^10.0.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.13",
    "express": "^4.21.2",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest-axe": "^10.0.0",
    "jsdom": "^24.1.1",
    "lightningcss": "^1.30.1",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.44",
    "postcss-import": "^16.1.0",
    "postcss-preset-env": "^10.1.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "rollup-plugin-visualizer": "^6.0.3",
    "size-limit": "^11.2.0",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^5.3.3",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^0.20.5",
    "vitest": "^2.1.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie 11"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
